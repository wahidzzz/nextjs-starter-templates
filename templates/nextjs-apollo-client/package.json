    "eslint-plugin-import": "2.25.4",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "18.0.10",
    "husky": "^8.0.3",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3"
