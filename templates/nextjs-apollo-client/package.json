{
  "name": "nextjs-apollo-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" && next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@next/font": "13.1.1",
    "apollo3-cache-persist": "^0.14.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redux-persist": "^6.0.0",
    "typescript": "4.9.4"
